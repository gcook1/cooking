.plaza-header-container {
  align-items: center;
  background-color: black;
  /*box-shadow: 0 4px 20px -2px rgba(255,255,255,.5);*/
  color: white;
  display: flex;
  height: 128px;
  width: 100%;
}

.plaza-header-navigation {
  margin: 0px 0px 0px 32px;
}

.plaza-header-title {
  font-size: 128px;
  margin: 0px auto;
  -webkit-user-select: none;
}

.plaza-header-logo {
  margin: 0px 32px 0px 0px;
}

.plaza-header-title a:link {
    text-decoration: none;
}
